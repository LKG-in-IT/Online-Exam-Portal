@model IEnumerable<OEP.Resources.Admin.ExamQuestionResource>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.QuestionId)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
