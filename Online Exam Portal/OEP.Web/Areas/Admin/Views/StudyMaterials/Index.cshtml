@model IEnumerable<OEP.Resources.Admin.StudyMaterialsResources>

@{
    ViewBag.Title = "StudyMaterials Listing";
    Layout = "~/Areas/Admin/Views/Shared/AdminLayout.cshtml";
    ViewBag.Page = "StudyMaterials";
    ViewBag.SubPage = "";
}


<div class="box">
    <div class="box-header">
        @Html.ActionLink("Create New", "Create", null, new { @class = "btn-success btn" })
    </div>
    <div class="box-body">
        <table id="examGrid" class="table table-striped table-bordered dt-responsive nowrap table-hover" cellspacing="0">
            <thead>
                <tr>
                    <th>SubCategory</th>
                    <th>File Name</th>
                    <th>File</th>
                    <th>Status</th>
                    <th>Edit</th>
                    <th>Details</th>
                    <th>Delete</th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>
<input type="hidden" id="hfRole" value="@(User.IsInRole("Admin")?"true":"false")" />
@section Scripts {
    <link href="~/Areas/Admin/Assets/dist/css/dataTables.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Areas/Admin/Assets/dist/css/responsive.bootstrap.min.css" rel="stylesheet" />

    <script src="~/Areas/Admin/Assets/dist/js/jquery.dataTables.min.js"></script>
    <script src="~/Areas/Admin/Assets/dist/js/dataTables.bootstrap4.min.js"></script>

    <script src="~/Areas/Admin/Assets/dist/js/custom/StudyMaterials.js"></script>
}

